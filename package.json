{
  "name": "stockscout",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "axios": "^1.3.4",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.7",
    "highcharts": "^10.3.3",
    "highcharts-react-official": "^3.2.0",
    "node-sass": "^8.0.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "react-slick": "^0.29.0",
    "react-virtualized": "^9.22.3",
    "redux": "^4.2.1",
    "slick-carousel": "^1.8.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint:js": "eslint **/*.{js,jsx,ts,tsx} --fix",
    "lint:format": "prettier **/*.{js,jsx,ts,tsx,json,css,scss,md,html} --write --ignore-unknown",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "cypress": "npx cypress open",
    "cypress-report": "npx nyc report --reporter=text-summary",
    "eject": "react-scripts eject",
    "analyze-window": "source-map-explorer build/static/js/*.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md,html}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "coverageReporters": [
      "text"
    ],
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs"
    },
    "clearMocks": true
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.12",
    "@testing-library/cypress": "^6.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^8.0.1",
    "babel-plugin-istanbul": "^6.0.0",
    "customize-cra": "^0.9.1",
    "cypress": "^4.7.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-testing-library": "^5.10.2",
    "husky": "^3.1.0",
    "istanbul-lib-coverage": "^3.0.0",
    "lint-staged": "^9.5.0",
    "nyc": "^15.0.1",
    "prettier": "^2.8.4",
    "react-app-rewired": "^2.1.11",
    "react-scripts": "^5.0.1",
    "react-test-renderer": "^16.14.0",
    "source-map-explorer": "^2.3.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
